# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# dependencies
/web/node_modules
/web/.pnp
/web.pnp.js

# testing
/web/coverage

# production
/web/build

# misc
.DS_Store
.env.local
.env.development.local
.env.test.local
.env.production.local

npm-debug.log*
yarn-debug.log*
yarn-error.log*
/web/src/env.js


# Rails

# Ignore bundler config.
/api/.bundle

# Ignore the default SQLite database.
/api/db/*.sqlite3
/api/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/api/log/*
/api/tmp/*
/api!/log/.keep
/api!/tmp/.keep

# Ignore uploaded files in development
/api/storage/*
/api!/storage/.keep

/api/.byebug_history

# Ignore master key for decrypting credentials and more.
/api/config/master.key
/api/vendor/ruby
/api/.env
